pico-8 cartridge // http://www.pico-8.com
version 7
__lua__

local game = {
  init = function(self)
    self.score = 0
    self.lives = 3
  end,
  draw = function(self)
    print(self.score, 4, 4, 15)
    for i = 1, self.lives do
      spr(0, 125 - i * 8, 3)
    end
  end
}

local paddle = {
  init = function(self)
    self.x = 52
    self.y = 122
    self.w = 24
    self.h = 4
  end,
  draw = function(self)
    rectfill(self.x, self.y, self.x + self.w, self.y + self.h, 15)
  end,
  update = function(self)
    if btn(0) then
      self.x -= 3
    elseif btn(1) then
      self.x += 3
    end
  end
}

local background = {
  draw = function(self)
    rectfill(0, 0, 128, 128, 3)
  end
}

local ball = {
  init = function(self)
    self.x = 64
    self.y = 64
    self.size = 3
    self.vx = 5
    self.vy = -3
  end,
  draw = function(self)
    circfill(self.x, self.y, self.size, 15)
  end,
  update = function(self)
    self.x += self.vx, self.y += self.vy

    if self.x < self.size then
      self.vx = -self.vx
      sfx(0)
    end

    if self.x > 128 - self.size then
      self.vx = -self.vx
      sfx(0)
    end

    if self.y < self.size then
      self.vy = -self.vy
      sfx(0)
    end

    if self.vy > 0 and self.x >= paddle.x and self.x <= paddle.x + paddle.w and self.y > paddle.y - paddle.h then
      self.vy = -self.vy
      game.score += 1
      sfx(0)
    end

    if self.y > 128 then
      game.lives -= 1
      if game.lives >= 0 then
        sfx(1)
        self.y = 24
      else
        sfx(2)
        _init()
      end
    end
  end
}

function _init()
  game:init()
  paddle:init()
  ball:init()
end

function _update()
  paddle:update()
  ball:update()
end

function _draw()
  background:draw()
  paddle:draw()
  ball:draw()
  game:draw()
end

