pico-8 cartridge // http://www.pico-8.com
version 7
__lua__

local game = {
  score = 0,
  lives = 5,
  draw = function(self)
    print(self.score, 4, 4, 15)
    for i = 1, self.lives do
      spr(0, 125 - i * 8, 3)
    end
  end
}

local paddle = {
  x = 52,
  y = 122,
  w = 24,
  h = 4,
  draw = function(self)
    rectfill(self.x, self.y, self.x + self.w, self.y + self.h, 15)
  end,
  update = function(self)
    if btn(0) then
      self.x -= 3
    elseif btn(1) then
      self.x += 3
    end
  end
}

local background = {
  draw = function(self)
    rectfill(0, 0, 128, 128, 3)
  end
}

local ball = {
  x = 64,
  y = 64,
  size = 3,
  vx = 5,
  vy = -3,
  draw = function(self)
    circfill(self.x, self.y, self.size, 15)
  end,
  update = function(self)
    self.x += self.vx, self.y += self.vy

    if self.x < self.size then
      self.vx = -self.vx
      sfx(0)
    end

    if self.x > 128 - self.size then
      self.vx = -self.vx
      sfx(0)
    end

    if self.y < self.size then
      self.vy = -self.vy
      sfx(0)
    end

    if self.vy > 0 and self.x >= paddle.x and self.x <= paddle.x + paddle.w and self.y > paddle.y - paddle.h then
      self.vy = -self.vy
      game.score += 1
      sfx(0)
    end

    if self.y > 128 then
      game.lives -= 1
      sfx(3)
      self.y = 24
    end
  end
}

local function _update()
  paddle:update()
  ball:update()
end

local function _draw()
  background:draw()
  paddle:draw()
  ball:draw()
  game:draw()
end

